# Track wallpaper binaries with Git LFS
scripts/dotfiles/wallpapers/*.png filter=lfs diff=lfs merge=lfs -text
scripts/dotfiles/wallpapers/*.jpg filter=lfs diff=lfs merge=lfs -text
scripts/dotfiles/wallpapers/*.jpeg filter=lfs diff=lfs merge=lfs -text
scripts/dotfiles/wallpapers/*.webp filter=lfs diff=lfs merge=lfs -text
scripts/dotfiles/wallpapers/*.bmp filter=lfs diff=lfs merge=lfs -text
